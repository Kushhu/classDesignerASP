        <!-- Modal -->
        <div class="modal fade user_auth" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                <div class="modal-content container">
                    <button type="button" class="close text-right close_modal" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <div class="wrap-login100 row">
                        <div class="login100-pic js-tilt col-lg-6 d-lg-block d-none">
                            <img src="img/login_img.png" alt="IMG" class="login_img" />
                        </div>

                        <div class="login100-form validate-form col-lg-6 col-12 pt-lg-5" id="pnl_login_user" runat="server">
                            <span class="login100-form-title">User Login
                            </span>

                            <div class="wrap-input100">
                                <asp:TextBox ID="txt_mobile_no_login" CssClass="input100" placeholder="Mobile Number" runat="server"></asp:TextBox>
                                <span class="focus-input100"></span>
                                <asp:RegularExpressionValidator ForeColor="Red" runat="server" ID="regx_txt_mobile_no_login" ControlToValidate="txt_mobile_no_login" ValidationGroup="login" ValidationExpression="^[0-9]{10}$" ErrorMessage="Enter Valid Contact Number" Display="Dynamic" />

                                <asp:RequiredFieldValidator ID="rf_txt_mobile_no_login" ForeColor="Red" runat="server" ErrorMessage="Please Fill Mobile No" ControlToValidate="txt_mobile_no_login" ValidationGroup="login" Display="Dynamic"></asp:RequiredFieldValidator>
                                <span class="symbol-input100">
                                    <i class="fa fa-mobile-phone" aria-hidden="true"></i>
                                </span>
                            </div>

                            <div class="wrap-input100 validate-input">
                                <asp:TextBox ID="txt_password_login" runat="server" TextMode="Password" CssClass="input100" placeholder="Password"></asp:TextBox>
                                <span class="focus-input100"></span>

                                <asp:RequiredFieldValidator ID="rf_txt_password_login" ForeColor="Red" runat="server" ErrorMessage="Please Fill Password" ControlToValidate="txt_password_login" ValidationGroup="login" Display="Dynamic"></asp:RequiredFieldValidator>
                                <span class="symbol-input100">
                                    <i class="fa fa-lock" aria-hidden="true"></i>
                                </span>
                            </div>

                            <div class="container-login100-form-btn">

                                <asp:Button ID="btnlogin" OnClick="btnlogin_Click" ValidationGroup="login" CssClass="login100-form-btn" runat="server" Text="Login" />
                            </div>

                            <div class="text-center p-t-12">
                                <span class="txt1"></span>
                                <a class="txt2" href="javascript:void(0);" onclick="btn_forgotpassword()">Forget Password?</a>
                            </div>

                            <div class="text-center p-t-136">
                                <a href="javascript:void(0);" onclick="btn_registration()">Create your Account
							            <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                                </a>
                            </div>
                        </div>

                        <asp:UpdatePanel ID="updtpnl_forget_password" runat="server">
                            <ContentTemplate>
                                <div class="myAlert-top alert alert-success" runat="server" id="div1">
                                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                    <strong runat="server" id="alert_text_fp"></strong>
                                </div>
                                <div class="login100-form validate-form pt-lg-5" id="pnl_forgot_password_user">
                                    <span class="login100-form-title">Forgot Password</span>
                                    <div class="wrap-input100">
                                        <asp:TextBox ID="txt_mobileno_fp" placeholder="Mobile No." CssClass="input100" runat="server"></asp:TextBox>
                                        <asp:RegularExpressionValidator runat="server" ForeColor="Red" ID="regx_txt_mobileno_fp" ControlToValidate="txt_mobileno_fp" Display="Dynamic" ValidationGroup="login" ValidationExpression="^[0-9]{10}$" ErrorMessage="Enter Valid Contact Number" />


                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-mobile-phone" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="wrap-input100 validate-input" id="pnl_otp_fp" style="display: none">
                                        <asp:TextBox ID="txt_otp_fp" placeholder="OTP Received On Registered Mobile Number" CssClass="input100" runat="server" ValidationGroup="login"></asp:TextBox>
                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-lock" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="wrap-input100 validate-input" id="pnl_password_fp" style="display: none">
                                        <asp:TextBox ID="txt_password_fp" placeholder="New Password" CssClass="input100" runat="server" ValidationGroup="login"></asp:TextBox>
                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-lock" aria-hidden="true"></i>
                                        </span>
                                    </div>


                                    <div class="container-login100-form-btn">
                                        <asp:Button ID="btn_forget_password" OnClick="btn_forget_password_Click" CssClass="login100-form-btn" runat="server" Text="Send OTP" />
                                    </div>
                                    <div class="text-center p-t-136">
                                        <a href="javascript:void(0);" onclick="btn_login()">Back To Login
							            <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                                        </a>
                                    </div>
                            </ContentTemplate>
                            <Triggers>
                                <asp:AsyncPostBackTrigger ControlID="btn_forget_password" EventName="Click" />
                            </Triggers>
                        </asp:UpdatePanel>
                    </div>


                    <div class="login100-form validate-form" id="pnl_register_user" runat="server">
                        <span class="login100-form-title">User Registration</span>

                        <asp:UpdatePanel ID="UpdatePanel1" runat="server">
                            <ContentTemplate>
                                <div class="myAlert-top alert alert-success" runat="server" id="div_alert">
                                    <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                                    <strong runat="server" id="alert_text"></strong>
                                </div>
                                <div id="pnl_registration_form">
                                    <div class="wrap-input100">
                                        <asp:TextBox ID="txt_name" CssClass="input100" placeholder="User Name" runat="server"></asp:TextBox>
                                        <asp:RequiredFieldValidator ID="rf_txt_name" runat="server" ErrorMessage="Please Fill Name" ControlToValidate="txt_name" ValidationGroup="save" Display="None"></asp:RequiredFieldValidator>
                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-user" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="wrap-input100">
                                        <asp:TextBox ID="txt_mob_no" CssClass="input100" placeholder="Mobile Number" runat="server"></asp:TextBox>
                                        <asp:RegularExpressionValidator runat="server" ID="regexp_txt_mob_no" ControlToValidate="txt_mob_no" Display="none" ValidationGroup="save" ValidationExpression="^[0-9]{10}$" ErrorMessage="Enter Valid Contact Number" />
                                        <asp:RequiredFieldValidator ID="rf_txt_mob_no" runat="server" ErrorMessage="Please Fill Mobile Number" ControlToValidate="txt_mob_no" ValidationGroup="save" Display="None"></asp:RequiredFieldValidator>
                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-mobile-phone" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="wrap-input100">
                                        <asp:TextBox ID="txt_referel_no" CssClass="input100" placeholder="Reference Number" runat="server"></asp:TextBox>
                                        <asp:RegularExpressionValidator runat="server" ID="re_txt_referel_no" ControlToValidate="txt_referel_no" Display="none" ValidationGroup="save" ValidationExpression="^[0-9]{10}$" ErrorMessage="Enter Valid Reference Number" />

                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-mobile-phone" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <%-- <div class="wrap-input100">
                                                <asp:TextBox ID="txt_email" CssClass="input100" placeholder="Email ID" runat="server"></asp:TextBox>
                                                <asp:RegularExpressionValidator ID="regexp_txt_email" runat="server" ValidationGroup="save" ControlToValidate="txt_email" ValidationExpression="^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$" Display="none" ErrorMessage="Invalid email address" />
                                                <span class="focus-input100"></span>
                                                <span class="symbol-input100">
                                                    <i class="fa fa-envelope" aria-hidden="true"></i>
                                                </span>
                                            </div>--%>
                                    <div class="wrap-input100 validate-input">
                                        <asp:TextBox ID="txt_password" runat="server" CssClass="input100" placeholder="Password"></asp:TextBox>
                                        <asp:RegularExpressionValidator ID="regexp_txt_password" runat="server" ControlToValidate="txt_password"
                                            ErrorMessage="Minimum password length is 6" Display="None" ValidationExpression="^([a-zA-Z0-9@#$%^&+=*]{6,30})$"
                                            ValidationGroup="save" />
                                        <asp:RequiredFieldValidator ID="rf_txt_password" runat="server" ErrorMessage="Please Fill Password" ControlToValidate="txt_password" ValidationGroup="save" Display="None"></asp:RequiredFieldValidator>
                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100">
                                            <i class="fa fa-lock" aria-hidden="true"></i>
                                        </span>
                                    </div>


                                </div>
                                <div id="pnl_otp_form" style="display: none">
                                    <div class="wrap-input100 validate-input" data-validate="Password is required" id="pnl_otp">
                                        <asp:TextBox ID="txt_otp" ValidationGroup="reg" CssClass="input100" runat="server" TextMode="Password" placeholder="OTP"></asp:TextBox>
                                        <asp:RequiredFieldValidator ID="rfotp" runat="server" ErrorMessage="Please Enter OTP" ControlToValidate="txt_otp" ValidationGroup="reg" Display="None"></asp:RequiredFieldValidator>
                                        <span class="focus-input100"></span>
                                        <span class="symbol-input100"><i class="fa fa-neuter" aria-hidden="true"></i></span>
                                    </div>
                                </div>
                                <div class="container-login100-form-btn">
                                    <asp:Button ID="btn_reg" OnClick="btn_reg_Click" ValidationGroup="save" CssClass="login100-form-btn" runat="server" Text="Send OTP" />
                                    <asp:ValidationSummary ID="vsreg" ValidationGroup="save" ShowMessageBox="True" ShowSummary="False" runat="server" />
                                </div>

                            </ContentTemplate>
                            <Triggers>
                                <asp:AsyncPostBackTrigger ControlID="btn_reg" EventName="Click" />

                            </Triggers>
                            <Triggers>
                            </Triggers>
                        </asp:UpdatePanel>


